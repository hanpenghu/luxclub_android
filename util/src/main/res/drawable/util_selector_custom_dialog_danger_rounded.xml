<?xml version="1.0" encoding="utf-8"?>
<!--自定义对话框_按钮：红色边框-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/util_custom_dialog_button_danger_pressed" />
            <stroke android:width="1dp" android:color="@color/util_custom_dialog_button_danger_edge" />
            <corners android:radius="@dimen/util_custom_dialog_button_corner_radius"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
	        <solid android:color="@color/util_custom_dialog_button_danger_disabled" />
            <stroke android:width="1dp" android:color="@color/util_custom_dialog_button_danger_disabled_edge" />
            <corners android:radius="@dimen/util_custom_dialog_button_corner_radius"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/util_custom_dialog_button_danger" />
            <stroke android:width="1dp" android:color="@color/util_custom_dialog_button_danger_edge" />
            <corners android:radius="@dimen/util_custom_dialog_button_corner_radius"/>
        </shape>
    </item>
</selector>