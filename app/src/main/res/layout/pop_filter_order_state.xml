<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingTop="45dp"
    android:id="@+id/viewLayout">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#151924"
    android:alpha="0.8"></RelativeLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        android:background="@color/white_transparent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/order_state1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/setting_item_background"
            android:padding="@dimen/layout_space">

            <TextView
                android:id="@+id/txt_customer_order_state1"
                style="@style/font_small_xx_white_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_customer_order_state1" />

            <include layout="@layout/include_space_hor" />

            <ImageView
                android:id="@+id/txt_is_check1"
                style="@style/font_small_xx_gray_deep_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/setting_view_arrow"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/layout_space" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_divider" />
        <LinearLayout
            android:id="@+id/order_state2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/setting_item_background"
            android:padding="@dimen/layout_space">

            <TextView
                android:id="@+id/txt_customer_order_state2"
                style="@style/font_small_xx_white_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_customer_order_state2" />

            <include layout="@layout/include_space_hor" />

            <ImageView
                android:id="@+id/txt_is_check2"
                style="@style/font_small_xx_gray_deep_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/setting_view_arrow"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/layout_space" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_divider" />
        <LinearLayout
            android:id="@+id/order_state3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/setting_item_background"
            android:padding="@dimen/layout_space">

            <TextView
                android:id="@+id/txt_customer_order_state3"
                style="@style/font_small_xx_white_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_customer_order_state3" />

            <include layout="@layout/include_space_hor" />

            <ImageView
                android:id="@+id/txt_is_check3"
                style="@style/font_small_xx_gray_deep_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/setting_view_arrow"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/layout_space" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_divider" />
        <LinearLayout
            android:id="@+id/order_state4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/setting_item_background"
            android:padding="@dimen/layout_space">

            <TextView
                android:id="@+id/txt_customer_order_state4"
                style="@style/font_small_xx_white_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_customer_order_state4" />

            <include layout="@layout/include_space_hor" />

            <ImageView
                android:id="@+id/txt_is_check4"
                style="@style/font_small_xx_gray_deep_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/setting_view_arrow"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/layout_space" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_divider" />
        <LinearLayout
            android:id="@+id/order_state5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/setting_item_background"
            android:padding="@dimen/layout_space">

            <TextView
                android:id="@+id/txt_customer_order_state5"
                style="@style/font_small_xx_white_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txt_customer_order_state5" />

            <include layout="@layout/include_space_hor" />

            <ImageView
                android:id="@+id/txt_is_check5"
                style="@style/font_small_xx_gray_deep_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/setting_view_arrow"
                android:layout_gravity="right"
                android:layout_marginRight="@dimen/layout_space" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/line_divider" />
    </LinearLayout>

</FrameLayout>